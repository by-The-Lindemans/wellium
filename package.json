    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/qrcode": "^1.5.5",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@vitejs/plugin-legacy": "^7.1.0",
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.2.0",
    "cypress": "^14.5.3",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
