{
  "name": "wellium",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "yws": "PORT=9001 node node_modules/y-webrtc/bin/server.js",
    "serve": "vite --host relay.localhost --port 8100",
    "dev": "concurrently -k --success first -n YWS,APP \"npm run yws\" \"npm run serve\"",
    "electron:start": "electron ./electron/main.cjs",
    "electron:dev": "concurrently -k --success first -n YWS,APP,ELC \"npm run serve\" \"npm run electron:start\"",
    "test": "jest",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test.e2e": "cypress run",
    "test.unit": "vitest",
    "lint": "eslint",
    "android:reinstall": "rm -rf dist && VITE_BUILD_TIME=$(date -u +%FT%TZ) vite build && npx cap sync android && adb uninstall com.lndmn.wellium || true && (cd android && ./gradlew clean :app:installDebug)"
  },
  "dependencies": {
    "@capacitor-community/electron": "^5.0.1",
    "@capacitor-community/native-audio": "^7.0.0",
    "@capacitor/android": "7.4.2",
    "@capacitor/app": "^7.0.2",
    "@capacitor/barcode-scanner": "^2.0.3",
    "@capacitor/core": "^7.4.2",
    "@capacitor/filesystem": "^7.1.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "7.4.2",
    "@capacitor/keyboard": "^7.0.2",
    "@capacitor/status-bar": "7.0.2",
    "@ionic/react": "^8.7.2",
    "@noble/curves": "^1.9.6",
    "@noble/hashes": "^1.8.0",
    "@types/react-router": "^5.1.20",
    "@types/react-router-dom": "^5.3.3",
    "automerge": "^0.14.2",
    "capacitor-secure-storage-plugin": "^0.11.0",
    "electron": "^37.2.6",
    "history": "^5.3.0",
    "html5-qrcode": "^2.3.8",
    "install": "^0.13.0",
    "ionicons": "^8.0.13",
    "jest-environment-jsdom": "^30.0.5",
    "libsodium-wrappers": "^0.7.15",
    "mlkem": "^2.3.1",
    "nanoid": "^5.1.5",
    "peerjs": "^1.5.5",
    "qrcode": "^1.5.4",
    "react": "^19.1.1",
    "react-dom": "19.1.1",
    "react-router": "^7.7.1",
    "react-router-dom": "^7.7.1",
    "y-webrtc": "^10.3.0",
    "yjs": "^13.6.27"
